<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="custDir" prefHeight="782.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BikeShopApp.custDirectoryController">
    <children>
                  <TableView fx:id="customer_list" layoutX="-2.0" layoutY="138.0" prefHeight="638.0" prefWidth="770.0">
                      <columns>
                          <TableColumn fx:id="col_1" prefWidth="79.0" text="Id">
                              <cellValueFactory>
                                  <PropertyValueFactory property="Customer_ID" />
                              </cellValueFactory>
                          </TableColumn>
                          <TableColumn fx:id="col_2" prefWidth="161.0" text="First Name">
                              <cellValueFactory>
                                  <PropertyValueFactory property="Customer_FirstName" />
                              </cellValueFactory>
                          </TableColumn>
            <TableColumn fx:id="col_3" prefWidth="176.0" text="Last Name" />
                          <TableColumn fx:id="col_4" minWidth="0.0" prefWidth="114.0" text="Phone">
                              <cellValueFactory>
                                  <PropertyValueFactory property="Customer_PhoneNumber" />
                              </cellValueFactory>
                          </TableColumn>
                          <TableColumn fx:id="col_5" maxWidth="168.0" prefWidth="168.0" text="Email">
                              <cellValueFactory>
                                  <PropertyValueFactory property="Customer_EmailAddress" />
                              </cellValueFactory>
                          </TableColumn>
                      </columns>
                  </TableView>
      <VBox layoutX="57.0" layoutY="10.0" prefHeight="75.0" prefWidth="200.0">
         <children>
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="200.0" text="Name" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <JFXTextField fx:id="filter_name" prefHeight="30.0" prefWidth="243.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </JFXTextField>
         </children>
      </VBox>
      <VBox layoutX="299.0" layoutY="15.0" prefHeight="75.0" prefWidth="209.0">
         <children>
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="200.0" text="Phone" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <JFXTextField fx:id="filter_phone" prefHeight="30.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </JFXTextField>
         </children>
      </VBox>
      <VBox layoutX="524.0" layoutY="8.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="200.0" text="Email" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <JFXTextField fx:id="filter_email" prefHeight="30.0" prefWidth="62.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXButton fx:id="btn_customers_clear" mnemonicParsing="false" prefHeight="35.0" prefWidth="200.0" text="Clear Filters">
               <font>
                  <Font size="14.0" />
               </font>
            </JFXButton>
         </children>
      </VBox>
      <JFXButton fx:id="btn_customers_apply" layoutX="58.0" layoutY="94.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="200.0" text="Apply Filters">
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
    </children>
</AnchorPane>
