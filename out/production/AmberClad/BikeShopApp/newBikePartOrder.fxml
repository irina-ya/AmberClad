            </padding>
        </HBox>
        <HBox>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="166.0" text="Order Amount : " />
            <JFXTextField fx:id="orderAmountTextField" alignment="TOP_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefWidth="100" />
            <padding>
                <Insets bottom="10.0" left="40.0" right="10.0" top="35.0" />
            </padding>
        </HBox>
        <HBox>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="167.0" text="Order Date : " />
            <JFXDatePicker fx:id="OrderDate" defaultColor="BLACK" nodeOrientation="LEFT_TO_RIGHT" prefWidth="290" />
            <padding>
                <Insets bottom="10.0" left="40.0" right="10.0" top="35.0" />
            </padding>
