                    <Font name="System Bold" size="43.0" />
                </font></Label>
            <padding>
                <Insets bottom="10.0" left="20.0" right="10.0" top="50.0" />
            </padding>
        </HBox>
        <HBox>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="166.0" text="Bike Manufacturer Name : " />
            <JFXTextField fx:id="bikeManufacturerTextField" alignment="TOP_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefWidth="480" />
            <padding>
                <Insets bottom="10.0" left="40.0" right="10.0" top="35.0" />
            </padding>
        </HBox>
        <HBox>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="166.0" text="Bike Part Name : " />
